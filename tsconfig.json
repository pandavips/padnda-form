{
  "include": [
    "env.d.ts",
    "src/**/*",
    "config/**/*",
    "vite.config.*"
, "eslint.config.js"  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "target": "ES2016",
    "skipLibCheck": true,
    "composite": true,
    "noEmit": true,
    "module": "ESNext",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "types": [
      "vite/client",
      "node",
    ],
  }
}
